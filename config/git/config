    # added = green bold
    # changed = red bold strike
    # untracked = cyan
    #branch = gray black bold ul

[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green

[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
    whitespace = red reverse

[icdiff]
	options = --highlight --line-numbers

[branch "develop"]
	rebase = true

[core]
	quotepath = false
[pull]
	rebase = true
[push]
